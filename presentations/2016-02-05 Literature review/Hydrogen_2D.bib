@article{Kreuer1995a,
abstract = {The elementary steps of proton conductivity in oxides, proton transfer and hydroxyl ion reorientation, are investigated and the anticipated HD isotope effects of their rates are compared to those of proton (deuteron) conductivity. It is concluded that the rate of proton diffusivity in many oxides is limited by proton transfer between fixed oxygen sites. This is facilitated by the thermal fluctuation of the oxygen ion separation. In the case of a rigid array of oxygen ions an attempt frequency close to that of the OH (OD) oscillator and a high activation enthalpy is expected. The effect of softening the OO vibration is to decrease the attempt frequency and activation barrier. Criteria for high diffusivities of protonic defects in oxides are formulated.},
author = {Kreuer, K},
doi = {10.1016/0167-2738(94)00265-T},
file = {:C$\backslash$:/Users/devonw/Google Drive/projects/mendeley/docs/Kreuer - 1995 - HD isotope effect of proton conductivity and proton conduction mechanism in oxides.pdf:pdf},
issn = {01672738},
journal = {Solid State Ionics},
keywords = {HD isotope effect,Oxide,Proton conductivity,hydrogen},
mendeley-tags = {hydrogen},
