keywords = {Computer Simulation,Hydrophobic and Hydrophilic Interactions,Hydroxides,Hydroxides: chemistry,Models, Chemical,Onium Compounds,Onium Compounds: chemistry,Quantum Theory,Surface Properties,Water,Water: chemistry,hydrogen},
mendeley-tags = {hydrogen},
