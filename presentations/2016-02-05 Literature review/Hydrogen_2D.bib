keywords = {graphene},
mendeley-tags = {graphene},
month = {oct},
pages = {1--10},
title = {{Impermeability of graphene and its applications}},
url = {http://www.sciencedirect.com/science/article/pii/S0008622313004880},
volume = {62},
year = {2013}
}
@article{Nguyen2015,
abstract = {Using density-functional calculations, we show that electron or hole doped graphene can strongly change the mobility of adsorbed atoms H and O. Interestingly, charge doping affects the diffusion of H and O in the opposite way, namely, electron doping increases/reduces while hole doping reduces/increases the diffusion barrier of H/O, respectively. Specifically, on neutral graphene the diffusion barriers of O and H are 0.74 and 1.01 eV, which are, upon a hole doping of {\$}+5.9\backslashtimes10{\^{}}{\{}13{\}}{\$} cm{\$}{\^{}}{\{}-2{\}}{\$}, 0.90 and 0.77 eV, and upon an electron doping of {\$}-5.9\backslashtimes10{\^{}}{\{}13{\}}{\$} cm{\$}{\^{}}{\{}-2{\}}{\$}, 0.38 and 1.36 eV, respectively. This means, within the harmonic transition state theory, at room temperature, the diffusion rate of O can be decreased or increased by 470 or 2.2{\$}\backslashtimes 10{\^{}}7{\$} times, and that of H can be increased or decreased by {\$}10{\^{}}5{\$} or {\$}7\backslashtimes 10{\^{}}7{\$} times, by that hole or electron doping level. The difference between the H and O cases is interpreted in terms of the difference in geometric and bonding changes upon charge doping.},
archivePrefix = {arXiv},
arxivId = {1512.02971},
author = {Nguyen, Manh-Thuong and Phong, Pham Nam},
eprint = {1512.02971},
file = {:C$\backslash$:/Users/devonw/Google Drive/projects/mendeley/docs/Nguyen, Phong - 2015 - Atomic transport at charged graphene why hydrogen and oxygen are so different.pdf:pdf},
keywords = {graphene,hydrogen,transport},
mendeley-tags = {graphene,hydrogen,transport},
month = {dec},
pages = {5},
title = {{Atomic transport at charged graphene: why hydrogen and oxygen are so different}},
url = {http://arxiv.org/abs/1512.02971},
year = {2015}
}
