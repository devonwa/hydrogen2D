month = {jan},
pages = {6539},
pmid = {25781149},
publisher = {Nature Publishing Group},
title = {{Aqueous proton transfer across single-layer graphene.}},
url = {http://www.nature.com/ncomms/2015/150317/ncomms7539/full/ncomms7539.html},
volume = {6},
year = {2015}
