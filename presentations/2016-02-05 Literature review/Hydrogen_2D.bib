month = {apr},
number = {6933},
pages = {705--7},
pmid = {12700757},
shorttitle = {Nature},
title = {{Dissociative hydrogen adsorption on palladium requires aggregates of three or more vacancies.}},
url = {http://dx.doi.org/10.1038/nature01557},
volume = {422},
year = {2003}
}
