file = {:C$\backslash$:/Users/devonw/Google Drive/projects/mendeley/docs/Gao et al. - 2014 - Ozonated graphene oxide film as a proton-exchange membrane.pdf:pdf},
issn = {1521-3773},
journal = {Angewandte Chemie (International ed. in English)},
keywords = {graphene},
mendeley-tags = {graphene},
month = {apr},
number = {14},
pages = {3588--93},
pmid = {24677748},
title = {{Ozonated graphene oxide film as a proton-exchange membrane.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/24677748},
volume = {53},
year = {2014}
}
